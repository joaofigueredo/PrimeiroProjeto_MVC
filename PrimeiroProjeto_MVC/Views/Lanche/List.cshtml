@model IEnumerable<PrimeiroProjeto_MVC.Models.Lanche>

<div> <h2>Todos os Lanches</h2> </div>
@foreach (var lanche in Model) {
        <div>
            <h4>Lanche : @lanche.NomeLanche</h4>
            <p><img style="height:255px; width:380px;" src="@lanche.ImagemUrl" /></p>
            <p>Preço : @lanche.Preco.ToString("c")</p>
        </div>
}
